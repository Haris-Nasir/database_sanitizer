{
  "name": "database_sanitizer",
  "version": "1.0.0",
  "description": "This directory contains multiple formats of your database dump for different tools and use cases.",
  "main": "create-custom-format-dump.js",
  "scripts": {
    "dbeaver-dump": "node create-custom-format-dump.js",
    "postgres-dump": "node sanitize-postgres-dump.js",
    "db-migrate": "node db-dump-restore.js",
    "anonymize": "node anonymize.js",
    "scheduler": "node database-scheduler.js",
    "scheduler:now": "node database-scheduler.js --now",
    "scheduler:start": "node database-scheduler.js",
    "scheduler:stop": "pkill -f database-scheduler.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haris-Nasir/database_sanitizer.git"
  },
  "keywords": [],
  "author": "",
