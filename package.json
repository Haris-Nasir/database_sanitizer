  "bugs": {
    "url": "https://github.com/Haris-Nasir/database_sanitizer/issues"
  },
  "homepage": "https://github.com/Haris-Nasir/database_sanitizer#readme",
  "type": "module",
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "dotenv": "^16.3.1",
    "node-cron": "^4.2.1"
  }
}
